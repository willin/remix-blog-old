{
  "private": true,
  "name": "willin.wang",
  "description": "R3mix of Willin.Wang",
  "license": "GPL-3.0",
  "scripts": {
    "build": "npm run build:css && cross-env NODE_ENV=production remix build",
    "dev": "cross-env NODE_ENV=development run-p dev:*",
    "build:css": "postcss styles/**/*.css --base styles --dir app/styles",
    "build:css:prod": "npm run build:css -- --env production",
    "postinstall": "remix setup cloudflare-pages",
    "dev:css": "npm run build:css -- --w",
    "dev:remix": "remix watch",
    "dev:wrangler": "wrangler pages dev ./public --watch ./build",
    "start": "npm run dev:wrangler",
    "lint": "eslint --fix --ext .ts,.tsx --ignore-path .gitignore .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@octokit/plugin-throttling": "^3.5.2",
    "@octokit/rest": "^18.12.0",
    "@remix-run/cloudflare-pages": "^1.1.1",
    "@remix-run/react": "^1.1.1",
    "@tailwindcss/typography": "^0.5.0",
    "classnames": "^2.3.1",
    "daisyui": "^1.20.1",
    "framer-motion": "^5.5.5",
    "i18next": "^21.6.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.15.3",
    "remix": "^1.1.1",
    "remix-i18next": "^2.1.5",
    "remix-themes": "^1.1.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.2.0",
    "@remix-run/dev": "^1.1.1",
    "@remix-run/serve": "^1.1.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@willin/eslint-config": "^0.0.6",
    "autoprefixer": "^10.4.1",
    "cross-env": "^7.0.3",
    "esbuild": "0.13.14",
    "eslint": "^8.5.0",
    "eslint-config-willin": "^1.0.7",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.8",
    "typescript": "^4.5.4",
    "wrangler": "pages"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "main": "dist/worker.js"
}
