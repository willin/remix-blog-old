{
  "private": true,
  "name": "willin.wang",
  "description": "R3mix of Willin.Wang",
  "author": "Willin Wang <willin@willin.org> (https://willin.wang/)",
  "license": "GPL-3.0",
  "scripts": {
    "compress": "node scripts/images.mjs",
    "build": "npm run build:prisma && npm run build:css && NODE_ENV=production remix build",
    "dev": "npm run build && pm2-dev ./pm2.config.js  --env development",
    "build:prisma": "PRISMA_CLIENT_ENGINE_TYPE=dataproxy prisma generate",
    "build:css": "postcss styles/**/*.css --base styles --dir app/styles --env production",
    "postinstall": "remix setup cloudflare-pages",
    "lint": "eslint --fix --ext .ts,.tsx --ignore-path .gitignore .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@prisma/client": "^3.9.2",
    "@remix-run/cloudflare-pages": "^1.2.1",
    "@remix-run/react": "^1.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.2.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.4.0",
    "@remix-run/dev": "^1.2.1",
    "@remix-run/eslint-config": "^1.2.1",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@willin/eslint-config": "^0.1.1",
    "autoprefixer": "^10.4.2",
    "esbuild": "^0.14.22",
    "eslint": "^8.9.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "pm2": "^5.1.2",
    "postcss": "^8.4.6",
    "postcss-cli": "^9.1.0",
    "prettier": "^2.5.1",
    "prisma": "^3.9.2",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.5",
    "wrangler": "^0.0.0-a73e32f"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "main": "build/index.js"
}
